CACHE MANIFEST
# <%= Date.today %>

CACHE:
/
<%= asset_path(:css, 'site') %>
<%= asset_path(:js, 'site') %>
<%= asset_path(:images, 'logo.svg') %>
<%= asset_path(:images, 'icon/icn-chevron-down.svg') %>
<%= asset_path(:images, 'icon/icn-map-pin.svg') %>
<% data.destinations.each do |destination_code, destination_data| %>
  <%= asset_path(:images, "bg-#{destination_data.slug}.jpg") %>
<% end %>

<% data.departures.each do |departure_code, departure_data| %>
  <% data.destinations.each do |destination_code, destination_data| %>
    <%= "/#{destination_data.slug}-from-#{departure_code.downcase}" %>
  <% end %>
<% end %>

NETWORK:
*


<!-- Language -->
<div class="item-title">
  <h1>Language</h1>
  <svg width="26px" height="14px">
    <use xlink:href="#caret-down"></use>
  </svg>
</div>
<section>
  <svg width="100%" height="75px">
    <use xlink:href="#slope-border"></use>
  </svg>
  <div class="item-content">
    <svg width="27px" height="29px">
      <use xlink:href="#icn-zap"></use>
    </svg>
    <h2>Languages</h2>
    <p><%= to.languages.join(', ') %></p>
    <svg width="27px" height="29px">
      <use xlink:href="#icn-zap"></use>
    </svg>
    <h2>Key Phrases</h2>
    <table>
      <tbody>
        <tr>
          <td>hello</td>
          <td>bonjour</td>
        </tr>
        <tr>
          <td>yes</td>
          <td>oui</td>
        </tr>
        <tr>
          <td>no</td>
          <td>non</td>
        </tr>
        <tr>
          <td>thank you</td>
          <td>merci</td>
        </tr>
        <tr>
          <td>excuse me</td>
          <td>pardonnez-moi</td>
        </tr>
      </tbody>
    </table>
  </div>
</section>

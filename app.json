{
  "name": "Upon Arrival",
  "description": "Everything you need to know the moment you touch down.",
  "repository": "https://github.com/sarahball/upon-arrival",
  "logo": "https://raw.githubusercontent.com/sarahball/upon-arrival/master/source/images/icon/logo.png",
  "keywords": ["middleman", "static"],
  "addons": [],
  "env": {
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "MEMCACHEDCLOUD_PASSWORD": {
      "required": true
    },
    "MEMCACHEDCLOUD_SERVERS": {
      "required": true
    },
    "MEMCACHEDCLOUD_USERNAME": {
      "required": true
    },
    "DARKSKY_API_KEY": {
      "required": true
    },
    "OPEN_EXCHANGE_RATE_API": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    },
    "worker": {
      "quantity": 0
    }
  },
  "addons": [
    "memcachedcloud",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/ruby"
    }
  ]
  
}
